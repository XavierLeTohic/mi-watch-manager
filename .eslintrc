{
    "plugins": [
        "babel",
        "react"
    ],
    "extends": [
      "prettier"
    ],
    "env": {
        "es6": true,
        "node": true
      },
      "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
      },
      "rules": {
        "prettier/prettier": "error"
      }
  }